<table>
  <thead>
    <tr>
      <th>| メッセージ相手</th>
      <th>| メッセージ内容</th>
    </tr>
  </thead>
  <tbody>
    <% @another_entries.each do |entry| %>
      <tr>
        <td><%= link_to entry.customer.email, customer_path(entry.customer) %></td>
        <td>
          <%= link_to room_path(entry.room) do %>
            <%= Message.find_by(id: entry.room.message_ids.last)&.body %>
          <%end%>
        </td>
      </tr>
    <%end%>
  </tbody>
</table>
<%= link_to "ユーザー一覧に戻る", customer_paths_path %>